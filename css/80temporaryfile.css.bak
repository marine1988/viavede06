/* Personal Combo */
.personalCombo {
    display: block;
    clear: both;
}

.personalCombo + .tooltip-icon {
    vertical-align: top;
    margin: 10px 0 0 0;
}

.form-theme-default .form-select-wrapper .personalCombo,
.form-label + .personalCombo {
    margin-top: 5px;
}

.tooltip-error.form-select .personalCombo,
.tooltip-info.form-select .personalCombo,
.tooltip-inquiry.form-select .personalCombo {
    display:    inline-block;
    clear:      none;
    width:      80%!important;
    vertical-align: middle;
}

.personalCombo dt {
    cursor: pointer;
    position: relative;
    display: block;
    clear: both;
    width: 100%;
    min-height: 50px;
    line-height: 50px;

    background: #f5f5f5;
    border: 0;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
}

.personalCombo dt .selectable {
    display: inline-block;
    vertical-align: middle;
    width: 100%;
    padding: 0 60px 0 20px;
    text-align: left;
    font-size: 1.4rem;
    line-height: 1.4;
    color: #495264;
    margin: 0;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}

.personalCombo dt .selectable strong{
    display: inline-block;
    margin: 0 5px 0 0;
    color: #495264;
}
.personalCombo dt .selectable:after {
    content: '';
    position: absolute;
    top: 0;
    right: 0;
    margin: 0;
    width: 42px;
    height: 100%;
    text-align: center;
    border-left: 2px solid #ffffff;
    font-size: 0;
    background: url(images/ddl-arrow-down.png) no-repeat center;
}

.personalCombo.open dt .selectable:after {
    background: url(images/ddl-arrow-up.png) no-repeat center;
}

.personalCombo dd {
    position: relative;
    display: block;
    clear: both;
    width: 100%;
}

.personalCombo dd ul{
    display: none;
    list-style: none;
    position: absolute;
    left: 0;
    top: 100%;
    width: 100%;
	max-height: 250px;
	overflow: auto; 
    margin: 5px 0 0 0;
    background: #f5f5f5;
    border: 0;
    z-index: 10;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
}

.personalCombo dd ul li{
    display: block;
    clear: both;
    list-style: none;
}


.personalCombo dd ul li .selectable{
  cursor: pointer;
  display: block;
  padding: 10px 20px;
  font-size: 1.4rem;
  line-height: 1.4;
  color: #385968;
  text-align: left;
  background: none;
}

.personalCombo dd ul li .selectable strong{
    display: inline-block;
    margin: 0 5px 0 0;
    color: #495264;
}

.personalCombo dd ul li .selectable.selected {
    color: #444444;
    background: #dddddd;
}

.personalCombo dd ul li:hover .selectable {
  color: #444444;
  background: #dddddd;
}

