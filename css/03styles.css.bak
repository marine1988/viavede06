/* Navigation Menu Mobile */

.nav-menu-mobile {
	cursor: pointer;
	font-size: 1.6rem;
	vertical-align: middle;
	display: none;
	position: relative;
	width: 30px;
	height: 47px;
}

.nav-menu-mobile:before {
	content: '';
	position: absolute;
	z-index: 10;
	top: 12px;
	right: 0;
	width: 100%;
	height: 4px;
	background: #c0c1c6;
	-webkit-box-shadow: 0 20px 0 0 #c0c1c6, 0 10px 0 0 #c0c1c6;
	-moz-box-shadow: 0 20px 0 0 #c0c1c6, 0 10px 0 0 #c0c1c6;
	box-shadow: 0 20px 0 0 #c0c1c6, 0 10px 0 0 #c0c1c6;
}

.nav-menu-mobile:after {
	content: '';
	display: none;
	position: absolute;
	z-index: 10;
	top: 22px;
	right: 0;
	width: 110%;
	height: 4px;
	background: #c0c1c6;
}

.nav-menu-mobile.active:before {
	top: 22px;
	width: 110%;
	-webkit-box-shadow: 0 0 0 0 #c0c1c6;
	-moz-box-shadow: 0 0 0 0 #c0c1c6;
	box-shadow: 0 0 0 0 #c0c1c6;
	-ms-transform: rotate(45deg);
	-webkit-transform: rotate(45deg);
	transform: rotate(45deg);
}

.nav-menu-mobile.active:after {
	display: block;
	-webkit-box-shadow: 0 0 0 0 #c0c1c6;
	-moz-box-shadow: 0 0 0 0 #c0c1c6;
	box-shadow: 0 0 0 0 #c0c1c6;
	-ms-transform: rotate(135deg);
	-webkit-transform: rotate(135deg);
	transform: rotate(135deg);
}

.mobile-version .nav-menu-mobile.active+.nav-menu {
	display: block;
	position: absolute;
	z-index: 1;
	right: 0;
	border-top: 1px solid #e6e6e6;
	padding: 10px 30px;
	width: 50%;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	background-color: #fff;
}

.mobile-version .nav-menu-mobile.active+.nav-menu ul:before {
	display: none;
}

.mobile-version .nav-menu-mobile.active+.nav-menu ul>li {
	font-size: 2.1rem;
	display: block;
	border-bottom: 1px solid #e6e6e6;
	padding-left: 0;
}

.mobile-version .nav-menu-mobile.active+.nav-menu ul>li:last-child {
	border-bottom: none;
}

.mobile-version .nav-menu-mobile.active+.nav-menu>ul>li:nth-child(5) {
	font-size: 1.8rem;
	display: inline-block;
	padding-top: 20px;
	padding-right: 0;
	padding-bottom: 10px;
}

.mobile-version .nav-menu-mobile.active+.nav-menu>ul>li:nth-child(5) a {
	padding: 10px 18px;
}

.mobile-version .nav-menu-mobile.active+.nav-menu ul>li a {
	padding: 15px 0;
}

.mobile-version .nav-menu-mobile.active+.nav-menu ul>li a:first-child:not(:last-child) {
	padding-right: 50px;
}

.mobile-version .nav-menu-mobile.active+.nav-menu .nav-menu-sub ul>li {
	font-size: 1.8rem;
	border-bottom: none;
}

.mobile-version .nav-menu-mobile.active+.nav-menu ul>li .ic-arrow {
	cursor: pointer;
	position: absolute;
	z-index: 1;
