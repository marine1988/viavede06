/* CSS OF WEBSITE STRUCTURE */

html,
body {
	margin: 0;
	border: none;
	padding: 0;
}

html,
body,
form,
.main-div {
	height: 100%;
}

html {
	font-size: 62.5%;
	background-color: #e6e6e6;
}

body {
	color: #495264;
	font-size: 1.6rem;
	font-family: Arial, 'Helvetica Neue', Helvetica, sans-serif;
	-moz-osx-font-smoothing: grayscale;
	line-height: normal;
	margin: 0;
	min-width: 320px;
	min-height: 540px;
	/*overflow-x: hidden;*/
}

a,
a:visited {
	color: #02b245;
	text-decoration: underline;
}

a:hover,
a:focus,
a:active {
	color: #02b245;
	text-decoration: none;
}

strong,
b {
	font-weight: 700;
}

small {
	font-size: 87.5%;
}

img {
	display: block;
	border: none;
}

fieldset,
legend {
	margin: 0;
	border: none;
	padding: 0;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}

legend {
	/*float: left;*/
	width: 100%;
}


/* este float esta' a comer a margem do elemente abaixo - se calhar e' necessario remover este float */

legend:after {
	content: '';
	display: table;
	clear: both;
}

input,
textarea,
select,
button {
	/*font-size: inherit;*/
	font-family: inherit;
	outline: none;
}

input:focus,
textarea:focus,
select:focus,
button:focus {
	outline: none;
}

textarea {
	border: none;
	resize: none;
	width: 100%;
}

span:focus,
div:focus {
	outline: none;
}

iframe {
	border: none;
}

[disabled] {
	cursor: not-allowed;
	color: #989a9f;
	background-color: #f5f5f5;
}

.hidden,
.hidden[hidden] {
	overflow: hidden;
	position: absolute;
	clip: rect(0 0 0 0);
	margin: -1px;
	border: none;
	padding: 0;
	width: 1px;
	height: 1px;
}

.no-scroll {
	overflow: hidden;
}


/*.mobile-version.no-scroll{position: fixed;}*/

.main-div {
	background-color: #e6e6e6;
}

#ControlBar_ControlPanel~.LoggedIn .main-div {
	padding-top: 54px;
}

p.darkgreen {
	color: #0f5222;
	line-height: 20px;
}

p.darkgreen a {
	color: #0f5222;
	font-weight: 700;
}

.green {
	color: #02b245;
}

.red {
	color: #e64f4f;
}

.highlight {
	color: #495264;
	padding: 0 4px;
	background-color: #feec9f;
}

.bg-green,
.bg-blue,
.bg-blue-dark {
	color: #fff;
}

.bg-white {
	background-color: #fff;
}

.bg-green {
	background-color: #02b245;
}

.bg-blue {
	background-color: #4eb7cd;
}

.bg-blue-dark {
	background-color: #363c4c;
}

.bg-light-grey {
	background-color: #f5f5f5;
}

.bg-grey {
	background-color: #ebebeb;
}

.select2-container {
	z-index: 1;
}

.message-success {
	background: rgba(0, 0, 0, 0) url(images/message-success.png) no-repeat scroll 0 0;
	padding-left: 90px;
}


/* Base */

.mainContent {
	margin-bottom: 65px;
}

.homepage .mainContent {
	margin-bottom: 0;
}

.wrapper {
	margin: 0 auto;
	max-width: 1280px;
	width: 100%;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}

.clearFloats:after {
	content: '';
	display: table;
	clear: both;
}


/* End Base */


/***************************/


/* Sticky Footer Technique */


/***************************/

.Sticky-Wrapper {
	clear: both;
	max-width: 1400px;
	min-height: 100%;
	width: 100%;
	margin: 0 auto;
}

.homepage .Sticky-Wrapper {
	max-width: 100%;
}

.Sticky-Wrapper:after {
	content: '';
	display: table;
	clear: both;
}

.my-viaverde .Sticky-Wrapper {
	display: table;
	height: 100%;
}

.my-viaverde .header .wrapper {
	max-width: 1400px;
}


/*******************************/


/* End Sticky Footer Technique */


/*******************************/


/* Accession Template */

.accession {}


/*.accession .main{margin: 0 0 0 60px;}*/


/***************************/


/* Sticky Footer Technique */


/***************************/

.accession .Sticky-Wrapper {
	margin: 0 auto -84px;
}


/* the bottom margin is the negative value of the footer's height */

.accession .Sticky-Wrapper:after,
.accession .footer {
	height: 84px;
}


/* .push must be the same height as .footer */

.accession .footer {
	padding: 0;
	border: none;
}

.accession .footer .footer-copy {
	margin: 0;
	padding: 33px 3.5%;
}


/*******************************/


/* End Sticky Footer Technique */


/*******************************/


/* End Accession Template */

.Sticky-Wrapper .loading {
	display: none;
	position: absolute;
	left: 0;
	top: 0;
	z-index: 2;
	width: 100%;
	height: 100%;
	opacity: 0.6;
	background: #fff url(images/modal-loading.gif) no-repeat center center;
}

.Sticky-Wrapper .loading span {
	display: none;
}


/* Main Wrapper */

.main-wrapper {
	clear: both;
	width: 100%;
	height: 100%;
}

.main-wrapper .main,
.main-wrapper .aside {
	height: 100%;
}

.main-wrapper .aside {
	display: none;
	width: 360px;
	vertical-align: top;
	padding: 35px 0 0 0;
	background: #009b3a;
}

.main-wrapper .aside .points-status {
	position: relative;
	display: block;
	margin-left: 7.77%;
	margin-right: 7.77%;
	font-size: 0;
}

.main-wrapper .aside .points-status .profile-my-viaverde-corner {
	display: none;
}

.main-wrapper .aside .points-status .points,
.main-wrapper .aside .points-status .status {
	display: inline-block;
	width: 50%;
	font-size: 1.6rem;
	color: #0f5222;
	vertical-align: top;
}

.main-wrapper .aside .points-status .label-title {
	display: block;
	clear: both;
	font-family: 'PrometoRegular';
	font-size: 2.1rem;
	color: #ffffff;
}

.main-wrapper .aside .points-status .label-title>sub {
	font-size: 65%;
}


/* End */


/* My Via Verde */

.my-viaverde {}

.my-viaverde .header:before {
	display: none;
}

.my-viaverde .main {
	vertical-align: top;
}

.my-viaverde .main .mainContent {
	width: 98.03%;
	margin-bottom: 0;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}

.my-viaverde .main-wrapper {
	display: table-row;
}

.my-viaverde .main-wrapper .main,
.my-viaverde .main-wrapper .aside {
	display: table-cell;
}

.my-viaverde .main-wrapper .aside {
	width: 280px;
}

.aside .points-status .label-text {
	display: block;
	clear: both;
	padding: 5px 0 0 0;
}


/* End */


/* Services Landingpage */

.landing-services-wrapper {
	display: block;
	clear: both;
}

.landing-services-wrapper .mainContent {
	margin-bottom: 20px;
}

.landing-services-table {
	display: table;
	width: 100%;
	clear: both;
	margin: 0 0 20px 0;
}

.landing-services-row {
	display: table-row;
	width: 100%;
	clear: both;
}

.landing-services-cell {
	display: table-cell;
	width: 50%;
	vertical-align: top;
}

.threePosTemplate .small-banner {
	margin: 20px 0 0 0;
}


/* End */


/* Via Verde Pontos */

.landing-vvpontos .gm-banner {
	margin-top: 60px;
}


/* End */


/* Menu My Via Verde */

.main-wrapper .aside .menu-myviaverde {
	display: block;
	clear: both;
	font-family: 'PrometoMediumItalic';
	padding: 0 7.77% 60px;
}

.main-wrapper .aside .menu-myviaverde ul {
	display: block;
	clear: both;
	margin: 0;
	padding: 0;
	list-style: none;
}

.main-wrapper .aside .menu-myviaverde ul>li {
	position: relative;
	display: block;
	clear: both;
	list-style: none;
	background: url(images/filler_menu_viaverde.gif) repeat-x left bottom;
}

.main-wrapper .aside .menu-myviaverde ul>li a {
	display: inline-block;
	padding: 18px 0;
	color: #0f5222;
	font-size: 2.0rem;
	line-height: 1.2;
	text-decoration: none;
	vertical-align: middle;
}

.main-wrapper .aside .menu-myviaverde ul>li a:first-child:not(:last-child) {
	max-width: 85%;
}

.touch .main-wrapper .aside .menu-myviaverde ul>li a:hover,
.main-wrapper .aside .menu-myviaverde ul>li a.active {
	color: #ffffff;
}

.main-wrapper .aside .menu-myviaverde ul>li .icon {
	display: inline-block;
	position: absolute;
	top: 18px;
	width: 25px;
	height: 25px;
	vertical-align: middle;
	margin: 0 0 0 10px;
	background: url(images/icon-alert-menu.png) no-repeat center center;
}


/* End */


/* Form */

.form {}

.form-legend {
	color: #02b245;
	font-size: 1.4rem;
	font-family: 'PrometoRegular';
}

.form-label {
	font-size: 1.4rem;
	vertical-align: middle;
	display: inline-block;
}

.form-label-list {
	font-size: 1.6rem;
	margin-left: 35px;
	margin-bottom: 10px;
	list-style: none;
}

.form-input {
	display: inline-block;
	border: 1px solid #dbdbdb;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px;
	padding: 12px 6px;
	width: 100%;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	background-color: #fff;
}

.form-input input,
.form-input textarea {
	font-size: 1.6rem;
	line-height: 1.2;
	margin: 0;
	border: none;
	-webkit-border-radius: 0;
	-moz-border-radius: 0;
	border-radius: 0;
	padding: 0;
	width: 100%;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	background-color: transparent;
	-webkit-appearance: none;
}

.form-time input {
	background-color: #f5f5f5;
	border: medium none;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
	border-radius: 4px;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	display: inline-block;
	font-size: 1.6rem;
	height: 44px;
	padding: 6px;
	position: relative;
	text-align: center;
	width: 42px;
	z-index: 1;
}

.label-time {
	display: inline-block;
	padding-left: 30px;
}

.label-time .form-label {
	padding: 0;
	width: auto !important;
}


/* End */


/*===== ALERT ===========*/

.alert {
	padding: 30px;
	background-color: #f8f8f8;
	margin: 0 auto;
	max-width: 1280px;
	margin-bottom: -30px;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
}

.alert .title {
	font-weight: 700;
	color: #5990C3;
	margin-bottom: 5px;
	padding-left: 60px;
	font-size: 1.6rem;
	line-height: 1.2;
}

.alert .description {
	padding-left: 60px;
	margin-bottom: 0;
	color: #495264;
	font-size: 1.6rem;
	line-height: 1.2;
}

.alert .alert-info {
	width: 40px;
	height: 40px;
	float: left;
	padding: 0;
}


