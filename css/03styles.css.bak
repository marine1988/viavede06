}


/* jQuery UI Accordion */

.accordion {
	color: #495264;
	font-size: 1.6rem;
	margin-bottom: 20px;
}

.accordion article {
	background-color: #f8f8f8;
	padding: 1px 0;
}

.accordion article:nth-child(even) {
	background-color: #efefef;
}

.accordion article>div {
	padding: 0 30px 30px;
}

.accordion h3 {
	background: url(images/ddl-arrow-down.png) no-repeat right 30px center;
	cursor: pointer;
	font-size: 1.6rem;
	margin: 0;
	outline: none;
	padding: 20px 60px 20px 30px;
}

.accordion h3:hover {
	color: #02b245;
}

.accordion h3.ui-accordion-header-active {
	background: url(images/ddl-arrow-up.png) no-repeat right 30px center;
	color: #02b245;
}

.accordion .accordion-content {
	padding-bottom: 20px;
	padding-right: 110px;
}

.accordion article p {
	line-height: 24px;
	/*margin-bottom: 46px;*/
}

.accordion .resposta-util {
	border-top: 1px solid #ced0d3;
	font-size: 1.4rem;
	line-height: 18px;
	margin: 0;
	padding: 30px 0 25px;
	text-align: right;
	width: 100%;
}

.accordion .resposta-util a {
	color: #495264;
	padding: 0 5px;
	text-decoration: none;
}

.accordion .resposta-util a:hover {
	color: #02b245;
}


/* End jQuery UI Accordion */


/* jQuery UI Auto Complete */

.autocomplete-container,
.extracts-autocomplete {
	position: relative;
}

.ui-autocomplete {
	cursor: default;
	position: absolute;
	z-index: 2;
	/*top: 0;left: -18px;width: 100%;*/
	overflow-y: auto;
	/* prevent horizontal scrollbar */
	overflow-x: hidden;
	list-style: none;
	text-transform: none;
	background: #ffffff;
	border: none;
	font-size: 1.6rem;
}

.autocomplete-container .ui-autocomplete,
.extracts-autocomplete .ui-autocomplete {
	left: -18px!important;
	width: 100%!important;
}

.ui-autocomplete li {
	padding: 10px;
	list-style: none;
}

.ui-autocomplete a {
	cursor: pointer;
	color: #495264;
}

.touch .ui-autocomplete li:hover {
	background: #02b245;
}

.touch .ui-autocomplete li:hover a {
	color: #ffffff;
}


/* End jQuery UI Auto Complete */


/* jQuery UI Datepicker */

.form-datepicker-wrapper {
	position: relative;
}

.form-datepicker {
	display: inline-block;
	margin-top: 5px;
	position: relative;
	width: 100%;
}

.form-datepicker input {
	background-color: #f5f5f5;
	border: medium none;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
	border-radius: 4px;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	display: inline-block;
	font-size: 1.6rem;
	height: 44px;
	padding: 6px;
	position: relative;
	width: 100%;
	z-index: 1;
}

.form-datepicker .datepicker-icon {
	background: #f5f5f5 url(images/ic-calendar.png) no-repeat scroll center center;
	-webkit-border-bottom-right-radius: 4px;
	border-bottom-right-radius: 4px;
	border-left: 2px solid #fff;
	-webkit-border-top-right-radius: 4px;
	border-top-right-radius: 4px;
	cursor: pointer;
	height: 44px;
	position: absolute;
	right: 0;
	top: 0;
	width: 42px;
	z-index: 1;
}


/* Erro de datepicker */

.tooltip-error.form-datepicker input.datepicker,
.tooltip-info.form-datepicker input.datepicker,
.tooltip-inquiry.form-datepicker input.datepicker,
.tooltip-error.form-datepicker input,
.tooltip-info.form-datepicker input,
.tooltip-inquiry.form-datepicker input {
	clear: none;
	display: inline-block;
	max-width: calc(100% - 43px)!important;
}

.tooltip-error.form-datepicker .datepicker-icon,
.tooltip-info.form-datepicker .datepicker-icon,
.tooltip-inquiry.form-datepicker .datepicker-icon {
	right: 43px;
}

.tooltip-close .tooltip-icon {
	background: rgba(0, 0, 0, 0) url("images/link-close.png") no-repeat scroll center center;
	display: inline-block;
	height: 28px;
	vertical-align: middle;
	width: 38px;
}


/* fim */

.ui-datepicker {
	background-color: #f5f5f5;
	color: #495264;
	position: absolute;
	text-align: center;
	top: -1000px;
	width: 288px;
}

.ui-datepicker-header {
	line-height: 50px;
}

.ui-datepicker-prev,
.ui-datepicker-next {
	background: url(images/ic-arrow-green.png) no-repeat center left;
	cursor: pointer;
	float: left;
	height: 26px;
	margin: 12px 0 0 15px;
	width: 26px;
}

.ui-datepicker-next {
	background: url(images/ic-arrow-green-right.png) no-repeat center right;
	float: right;
	margin: 12px 15px 0 0;
}

.ui-datepicker-prev span,
.ui-datepicker-next span {
	display: none;
}

.ui-datepicker-title {
	font-family: 'PrometoRegular';
	font-size: 1.8rem;
}

.ui-datepicker-calendar {
	border-collapse: collapse;
	width: 288px;
}

.ui-datepicker-calendar thead {
	background-color: #02b245;
	color: #fff;
	line-height: 40px;
}

.ui-datepicker-calendar thead th {
	font-weight: 400;
}

.ui-datepicker-calendar tbody td {
	border: 1px solid #dadadc;
	line-height: 30px;
	width: 40px;
}

.ui-datepicker-calendar tbody td a {
	color: #495264;
	text-decoration: none;
}

.ui-datepicker-calendar td.ui-datepicker-current-day:before,
.ui-datepicker-calendar td.ui-datepicker-days-cell-over:before {
	border-color: transparent #495264 transparent transparent;
	border-style: solid;
	border-width: 0 7px 7px 0;
	height: 0;
	content: '';
	position: absolute;
	right: -1px;
	top: -1px;
	width: 0;
}

.ui-datepicker-calendar td.ui-datepicker-current-day,
.ui-datepicker-calendar td.ui-datepicker-days-cell-over {
	border: 2px solid #495264;
	position: relative;
}

.ui-datepicker-calendar td.ui-datepicker-other-month,
.ui-datepicker-calendar td.ui-datepicker-other-month a {
	color: #cbcbcb;
}

.ui-datepicker-calendar td.ui-datepicker-unselectable,
.ui-datepicker-calendar td.ui-datepicker-unselectable a {
	background-color: #eee;
	color: #cbcbcb;
}

.ui-datepicker-group {
	float: left;
	width: 33%;
}

.ui-datepicker-calendar td.ui-datepicker-days-cell-over.ui-datepicker-today:before {
	border: none;
}

.ui-datepicker-calendar td.ui-datepicker-today {
	background-color: #02b245;
	position: relative;
}

.ui-datepicker-calendar td.ui-datepicker-today:after {
	border-style: solid;
	border-width: 0 5px 5px;
	border-color: transparent transparent #1b8539 transparent;
	bottom: -1px;
	content: '';
	height: 0;
	left: 50%;
	margin-left: -4px;
	position: absolute;
	width: 0;
}

.ui-datepicker-calendar td.ui-datepicker-today a,
.ui-datepicker-calendar td.ui-datepicker-today span {
	color: #fff;
}


/* End jQuery UI Datepicker */


/* VenoBox */

.vbox-preloader {
	position: fixed;
	width: 128px;
	height: 15px;
	left: 50%;
	top: 50%;
	margin-left: -64px;
	margin-top: -7px;
	background-image: url(images/modal-loading.gif);
	text-indent: -100px;
	overflow: hidden;
}

.vbox-container img {
	display: inline-block;
}


/* End VenoBox */


/* Links and Buttons */

.link,
.button {
	cursor: pointer;
}

.link {
	color: #363c4c;
	text-decoration: none;
	vertical-align: middle;
	display: inline-block;
	margin-top: 3.2%;
	margin-bottom: 3.2%;
	background-color: transparent;
}

.button {
	color: #fff;
	text-decoration: none;
	text-transform: uppercase;
	vertical-align: middle;
	display: inline-block;
	overflow: hidden;
	margin: 0;
	border: none;
	-webkit-border-radius: 4px;
