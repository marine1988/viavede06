.cleanStyle {
	line-height: 1.6;
}

.cleanStyle img {
	max-width: 100%;
	display: inline-block;
}

.cleanStyle h1,
.cleanStyle h2,
.cleanStyle h3,
.cleanStyle h4,
.cleanStyle h5,
.cleanStyle h6 {
	line-height: 1;
	margin: 0 0 20px 0;
}

.cleanStyle h1 {
	font-family: 'PrometoMediumItalic';
	font-size: 4.8rem;
	color: #272b36;
	font-weight: normal;
}

.cleanStyle h2 {
	font-family: 'PrometoRegular';
	font-size: 3.6rem;
	color: #272b36;
	font-weight: normal;
}

.cleanStyle h3 {
	font-family: 'PrometoRegular';
	font-size: 2.6rem;
	color: #272b36;
	font-weight: normal;
}

.cleanStyle h4 {
	font-family: 'PrometoRegular';
	font-size: 2.2rem;
	color: #272b36;
	font-weight: normal;
}

.cleanStyle h5 {
	font-family: 'PrometoRegular';
	font-size: 1.8rem;
	color: #272b36;
	font-weight: normal;
}

.cleanStyle h6 {
	font-family: 'PrometoRegular';
	font-size: 1.6rem;
	color: #272b36;
	font-weight: normal;
}

.cleanStyle strong {
	font-weight: 700;
}


/* anchor style */

.cleanStyle a,
.cleanStyle a:active {
	color: #02b245;
	text-decoration: none;
}

.touch .cleanStyle a:hover {
	text-decoration: underline;
}

.touch .cleanStyle a.button:hover {
	text-decoration: none;
}


/* paragraph style */

.cleanStyle p {
	margin: 0 0 20px 0;
}

.cleanStyle p:empty {
	margin: 0;
}

.cleanStyle hr {
	clear: both;
	height: 1px;
	border: none;
	background: #a9abb1;
	line-height: 1px;
	margin: 20px 0;
	float: left;
	width: 100%;
}

.cleanStyle mark {
	color: #495264;
	padding: 0 4px;
	background-color: #feec9f;
}


/* UnOrdered List */

.cleanStyle ul {
	list-style: none;
	margin: 0 0 20px 0;
	counter-reset: decimal-bullet;
}

.cleanStyle ul li {
	position: relative;
	margin-left: 20px;
	margin-bottom: 8px;
	vertical-align: top;
}

.cleanStyle ul>li:before,
.cleanStyle ol ul>li:before {
	content: '';
	position: absolute;
	left: -17px;
	top: 8px;
	-webkit-border-radius: 8px;
	-moz-border-radius: 8px;
	border-radius: 8px;
	width: 8px;
	height: 8px;
	background-color: #02b245;
}

.cleanStyle ul ul,
.cleanStyle ul ol {
	margin-top: 8px;
	margin-left: 8px;
}


/* Ordered List */

.cleanStyle ol {
	list-style: none;
	margin: 0 0 20px 5px;
	counter-reset: decimal-bullet;
}

.cleanStyle ol li {
	position: relative;
	margin-left: 0;
	margin-bottom: 8px;
	vertical-align: top;
	counter-increment: decimal-bullet;
}

.cleanStyle ol li:first-child {
	margin-top: 0;
}

.cleanStyle ol>li:before,
.cleanStyle ul ol>li:before {
	content: counter(decimal-bullet) '.';
	color: #02b245;
	font-weight: 700;
	display: inline-block;
	margin-left: -5px;
	margin-right: 0.3em;
}

.cleanStyle ol ol,
.cleanStyle ol ul {
	margin-top: 8px;
	margin-left: 30px;
}


/* table style */


/* Default Table Style */

.cleanStyle table {
	width: 100%;
	margin: 0 0 20px 0;
	border-collapse: collapse;
	border-spacing: 0;
	background-color: transparent;
}

.cleanStyle table th,
.cleanStyle table td {
	text-align: center;
	height: 60px;
	padding: 0 20px;
	vertical-align: middle;
	font-weight: normal;
	border-right: solid 4px #ffffff;
}

.cleanStyle table thead th {
	font-size: 1.8rem;
	color: #ffffff;
	background: #02b245;
}

.cleanStyle table thead th:first-child {
	background: none;
}

.cleanStyle table tbody th {
	font-family: 'PrometoMedium';
	font-size: 1.6rem;
	color: #495264;
	text-align: left;
}

.cleanStyle table tbody td {
	font-family: 'PrometoRegular';
	font-size: 1.8rem;
	color: #495264;
}

.cleanStyle table tbody tr:nth-child(odd) th,
.cleanStyle table tbody tr:nth-child(odd) td {
	background: #efefef;
}

.cleanStyle table tbody tr:nth-child(even) th,
.cleanStyle table tbody tr:nth-child(even) td {
	background: #f8f8f8;
}