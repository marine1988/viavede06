/* MODULES */
.form-management-reference .contextual-modal fieldset {max-height: 300px;overflow: auto;-webkit-overflow-scrolling: touch;overflow-scrolling: auto;}

/* lnk-back-top */
.lnk-back-top{text-align: right;clear: both; padding: 30px 20px;}
.lnk-back-top .link{position: relative; display: inline-block; color: #272b36; margin: 0;}
.lnk-back-top .link:before{content: '';position: absolute;left: -35px;top: -4px;-webkit-border-radius: 26px;-moz-border-radius: 26px;border-radius: 26px;width: 26px;height: 26px;background-color: #02b245;}
.lnk-back-top .link .link-txt:before{content: '';position: absolute;left: -27px;top: 5px;width: 11px;height: 8px;background: transparent url(images/ic-arrow01.png) no-repeat left top;}

/******************/
/* column-section */
/******************/
.column-section {display: block;clear: both;}
.column-header {display: block;clear: both;padding: 60px 0 0 0;}
.column-header .page-title {display: block;clear: both;margin: 0 25px 50px 25px;padding: 0;font-family: 'PrometoMediumItalic';font-size: 4.8rem;font-weight: normal;line-height: 1;color: #363c4c;}
.column-section .column-body {display: block;clear: both;padding: 50px 5.2%;background: #ffffff;}
.column-section .column-body.noPadding {padding: 0;}
.column-section .column-body.cleanBody {padding: 0;background: none;}
.column-section .column-aside {display: block;clear: both;padding: 30px 0 35px 0;}

/**********************/
/* Fim column-section */
/**********************/

/* accession-wrapper */
.accession-wrapper {display: block;clear: both;}
.accession-header {display: block;clear: both;padding: 50px 3.5% 0;text-align: center;}

/*.accession-header h1.page-title {*/
.accession-header .page-title {display: block;clear: both;font-family: 'PrometoMediumItalic';font-size: 4.8rem;font-weight: normal;line-height: 1;color: #02b245;margin: 0;}
.accession-header .page-lead {display: block;clear: both;padding: 15px 0 0 0;font-size: 1.6rem;font-weight: normal;line-height: 1;color: #495264;}
.accession-panel {position: relative;display: block;clear: both;margin: 45px 0 40px 0;}
.accession-panel:after {content: "";display: table;clear: both;}

/* accession-main */
.accession-panel .accession-main {float: left;width: 70.14%;background: #ffffff;}
.accession-panel .accession-main.big {width: 100%;clear: both;}
.accession-panel .accession-main.big + .accession-aside {display: none;}
.accession-panel .accession-main .column-article .column-header {padding-top: 0;}
.accession-panel .accession-main .column-article .column-body:first-child {padding-top: 0;}
.accession-panel p{}
.wizardSteps {display: block;clear: both;}
.wizardSteps ul {display: block;clear: both;font-size: 0;list-style: none;margin: 0;padding:0}
.wizardSteps li {display: inline-block;width: 25%;vertical-align: top;}
.wizardSteps.two-steps li {width: 50%;}
.wizardSteps li a {cursor: default;position: relative;display: block;height: 47px;font-family: 'PrometoRegular';font-size: 1.4rem;line-height: 1;padding: 33px 0 0 20px;color: #0f5222;text-decoration: none;text-align: center;background: #23bb46;}
.wizardSteps li:first-child a {padding: 33px 0 0 0;}
.wizardSteps li a:after {content: " ";display: block;width: 0;height: 0;border-top: 40px solid transparent;
/* Go big on the size, and let overflow hide */
border-bottom: 40px solid transparent;border-left: 20px solid #23bb46;position: absolute;top: 50%;margin-top: -40px;left: 100%;z-index: 2;}
.wizardSteps li a:before {content: " ";display: block;width: 0;height: 0;border-top: 40px solid transparent;border-bottom: 40px solid transparent;border-left: 20px solid #ffffff;position: absolute;top: 50%;margin-top: -40px;margin-left: 1px;left: 100%;z-index: 1;}
.wizardSteps li:last-child a:after,
.wizardSteps li:last-child a:before {display: none;}
.wizardSteps li.current a,
.wizardSteps li.completed a {cursor: pointer;}
.wizardSteps li.current a {color: #ffffff;background: #009b3a;}
.wizardSteps li.current a:after {border-left-color: #009b3a!important;}
.wizardSteps li.completed a {color: #009b3a;background: #dbdbdb;}
.wizardSteps li.completed a:after {border-left-color: #dbdbdb!important;}
.wizardSteps li a > span {display: inline-block;width: 90%;}
.wizardSteps li a > span:before {content: '';vertical-align: middle;display: inline-block;width: 0;height: 100%;}
.wizardSteps li a > span > span {vertical-align: middle;display: inline-block;max-width: 95%;padding: 1% 0 1% 5%;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
.accession-panel .wizardStep{display: none;}
.accession-panel .accession-main .wizardStep {clear: both;padding: 50px 5.2%;}
.adhesion-main-container .accession-panel .accession-main .wizardStep {display: block;} /* Added display: block; because we are now using AngularJS to Show/Hide this element */
.accession-panel .accession-main .wizardStep.noLateralPadding {padding-left: 0;padding-right: 0;}
.accession-panel .accession-main .wizardStep fieldset {padding: 0;margin: 0;}
.form-theme-default .form-group .form-group-wrapper .column68 {display: inline-block;width: 68%;margin-top: 30px;}
.form-theme-default .form-group .form-group-wrapper .column30 {display: inline-block;width: 30%;margin-top: 30px;}
.accession-panel .accession-main .additionalCost {display: block;clear: both;padding: 30px 0 0 0;font-size: 1.6rem;color: #363c4c;}
.accession-panel .accession-main .additionalCost .cost {font-family: 'PrometoRegular';font-size: 3.2rem;color: #23bb46;vertical-align: middle;}
.accession-panel .accession-main .additionalCost .feed {font-family: 'PrometoRegular';font-size: 2rem;color: #363c4c;vertical-align: middle;text-transform: uppercase;}
.accession .form-theme-default .form-group-nif-country .form-pre-filled {padding-top: 0;}
.accession .form-theme-default legend {/*float: none;clear: both;width: 100%;*/} /* foi retirado o float para nao comer a margem do elemente abaixo - esta propriedade esta' a esmagar o float do ficheiro 03styles.css */
.accession .form-theme-default .form-legend {/*float: left;display: inline-block;clear: none;*/} /* foi retirado todas as propriedades porque nao esta' a fazer nada */
.accession .form-theme-default .form-legend.legend-final {color: #02b245;}
.accession .form-theme-default .mandatory-fields {display: block;clear: both;font-size: 1.6rem;padding-top: 10px;}
.desktop-version .accession-panel .accession-main .form-theme-default .block-info{width: 60%;}

/**
* Service Plan
*/
.service-plan {display: block;clear: both;text-align: center;font-size: 0;margin: 40px 0 60px 0;}
.service-plan .service-plan-item-wrapper {position: relative;display: inline-block;width: 29%;margin: 0 1%;vertical-align: middle;}
.service-plan .service-plan-item-wrapper > a {text-decoration: none;}
.service-plan .service-plan-item {position: relative;display: inline-block;width: 100%;vertical-align: middle;background: #ffffff;}
.service-plan .service-plan-item-wrapper .service-plan-item + .link {color: #a9abb1;}
.service-plan .service-plan-item label {cursor: pointer;display: block;clear: both;padding: 0 0 60px 0;}
.service-plan .service-plan-item .card-indication {display: inline-block;position: relative;clear: both;padding: 7px 23px 8px;min-width: 40%;font-family: 'PrometoRegular';font-size: 1.5rem;text-transform: uppercase;color: #fff;}

.service-plan .service-plan-item .card-wrapper {margin-top: -34px;}
.service-plan .service-plan-item .card-wrapper:first-child {margin-top: 0;}

.service-plan .service-plan-item .card-indication:before,
.service-plan .service-plan-item .card-indication:after {content: '';position: absolute;top: 0;width: 23px;height: 35px;background-image: url(images/service-plan-curve.png);background-repeat: no-repeat;-webkit-backface-visibility: hidden;}
.service-plan .service-plan-item .card-indication:before {left: 0;background-position: left top;}
.service-plan .service-plan-item .card-indication:after {right: 0;background-position: right top;}
/*.service-plan .service-plan-item .popular,
.service-plan .service-plan-item .type_2,
.service-plan .service-plan-item .new,
.service-plan .service-plan-item .type_3 {display: block;clear: both;line-height: 60px;font-family: 'PrometoRegular';font-size: 1.2rem;text-transform: uppercase;color: #fff;}*/
.service-plan .service-plan-item .card-indication.popular,
.service-plan .service-plan-item .card-indication.type_2,
.service-plan .service-plan-item .card-indication.new,
.service-plan .service-plan-item .card-indication.type_3 {display: inline-block;line-height: normal;font-size: 1.5rem;color: #fff;}
.service-plan .service-plan-item .popular,
.service-plan .service-plan-item .type_2 {background: #02b245;}
.service-plan .service-plan-item .new,
.service-plan .service-plan-item .type_3 {background: #0f5222;}

.service-plan .service-plan-item .card-wrapper {display: table;clear: both;width: 100%;}
.service-plan .service-plan-item .card-row {display: table-row;clear: both;width: 100%;height: 100%;}
.service-plan .service-plan-item .card-cell {display: table-cell;clear: both;width: 100%;height: 100%;vertical-align: middle;padding: 55px 2% 20px;-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}
.service-plan .service-plan-item .card-wrapper .plan-name {display: block;clear: both;padding: 0;font-family: 'PrometoRegular';font-size: 2.8rem;text-transform: uppercase;line-height: 1;color: #363c4c;}
.service-plan .service-plan-item .card-wrapper .plan-cost {position: relative;display: inline-block;clear: both;margin: 5px 0 0 0;font-family: 'PrometoRegular';font-size: 8.0rem;color: #02b245;line-height: 1;vertical-align: middle;}
.service-plan .service-plan-item .card-wrapper .plan-cost.old {margin: 5px 10px 0 0;font-size: 4.8rem;color: #a9abb1;}
.service-plan .service-plan-item .card-wrapper .plan-cost.old:before {content: '';position: absolute;left: -10%;top: 0;width: 115%;height: 47%;border-bottom: 3px solid #a9abb1;}
.service-plan .service-plan-item .card-wrapper .plan-cost.crossed .plan-cost-wrapper {position: relative;display: inline-block;padding: 0 20px;}
.service-plan .service-plan-item .card-wrapper .plan-cost.crossed .plan-cost-wrapper:before {content: '';position: absolute;left: 0;top: 0;width: 100%;height: 100%;z-index: 1;background: url(images/crossed-out.png) no-repeat center;background-size: contain;}
.service-plan .service-plan-item .card-wrapper .plan-cost sup{top: -0.75em;font-size: 45%;font-family: 'PrometoRegular';}
.service-plan .paperIconMessage,
.service-plan .service-plan-item .card-wrapper .plan-cost sup .icon {display: none;}
.service-plan .service-plan-item .card-wrapper .plan-cost .feed {font-family: 'PrometoRegular';font-size: 2rem;color: #363c4c;line-height: 2;vertical-align: bottom;margin-left: -1.8rem;}
.service-plan .service-plan-item .card-wrapper .plan-feed {display: block;clear: both;padding: 10px 5% 0;font-size: 1.5rem;color: #363c4c;line-height: 1.2;min-height: 40px;}
.service-plan .service-plan-item .card-wrapper .plan-feed sup {font-size: 65%;font-weight: 700;color: #02b245;}
.service-plan .service-plan-item .card-wrapper .plan-offers {display: inline-block;clear: both;list-style: none;margin: 20px 10% 0 10%;text-align: left;}
.service-plan .service-plan-item .card-wrapper .plan-offers li {display: block;clear: both;list-style: none;padding: 0 0 0 25px;margin: 0 0 5px 0;font-family: Arial, sans-serif;font-size: 1.6rem;color: #a9abb1;background: url(images/correct-green.png) no-repeat left 5px;}
.service-plan.paper .service-plan-item .card-wrapper .plan-offers li,
.service-plan.paper .service-plan-item.active .card-wrapper .plan-offers li {background: url(images/cross.png) no-repeat left 5px;}
.service-plan.paper .service-plan-item-wrapper.InLeve .card-wrapper .plan-offers li,
.service-plan.paper .service-plan-item-wrapper.InLeve.active .card-wrapper .plan-offers li {background: url(images/correct-green.png) no-repeat left 5px;}
.service-plan .service-plan-item.active .card-wrapper .plan-offers li {color: #0f5222;background: url(images/correct-white.png) no-repeat left 5px;}

.service-plan .service-plan-item .collaps-content {display: none;clear: both;/*border-top: 1px solid #dedfe0;*/}
.service-plan .service-plan-item .collaps-content-wrapper {margin: 0 10%;}
/*.service-plan .service-plan-item .collaps-content .service-plan-info {margin: 0 -12.5%;}*/
.service-plan .service-plan-item .collaps-content ul {display: block;clear: both;padding: 0;margin: 0;list-style: none;text-align: left;min-height: 155px;}
.service-plan .service-plan-item .collaps-content ul li {display: block;clear: both;list-style: none;padding: 15px 0 15px 0;font-size: 1.5rem;color: #363c4c;line-height: 1;font-weight: bold;border-bottom: 1px solid #dedfe0;}
.service-plan .service-plan-item .collaps-content ul li.send:first-child {border-top: 1px solid #dedfe0;}
.service-plan .service-plan-item .collaps-content ul li.send {padding: 15px 45px 15px 0;background: url(images/ic-send.png) no-repeat right center;}
.service-plan .service-plan-item .collaps-content ul li.send sup {font-size: 65%;font-weight: 700;color: #02b245;}
.service-plan .service-plan-item .collaps-content ul li.warranty {padding: 15px 45px 15px 0;background: url(images/ic-warranty.png) no-repeat right center;}
.service-plan .service-plan-item .collaps-content ul li.campaign {padding: 15px 45px 15px 0;background: url(images/ic-campaign.png) no-repeat right center;}
.service-plan .service-plan-item .collaps-content .button-wrapper {padding: 12% 0;}
.service-plan .service-plan-item .collaps-content .block-collaps-content {display: block;clear: both;padding: 0 0 15px 0;text-align: left;border-top: 1px solid #dedfe0;border-bottom: 1px solid #dedfe0;}
.service-plan .service-plan-item .collaps-content .block-collaps-title {display: block;clear: both;font-size: 1.6rem;color: #363c4c;padding: 15px 0 0 0;}
.service-plan .service-plan-item .collaps-content .block-collaps-text {display: block;clear: both;font-size: 1.4rem;color: #a9abb1;padding: 5px 0 0 0;}
.service-plan .service-plan-item .collaps-content .block-collaps-text p {margin: 0;}
.service-plan .service-plan-item input[type="radio"] {display: none;}
.service-plan .service-plan-item .collaps-arrow {cursor: pointer;display: block;/*margin-top: -30px;*/width: 100%;height: 60px;background: url(images/ic-collaps-arrow-down.png) no-repeat center;}
.service-plan .service-plan-item .collaps-arrow.open {background: url(images/ic-collaps-arrow-up.png) no-repeat center;}
.service-plan .service-plan-info.type_1 {background-color: #a9abb1;}
.service-plan .service-plan-info.type_2 {background-color: #02b245;}
.service-plan .service-plan-info.type_3 {background-color: #0f5222;}
.service-plan .service-plan-info {color: #fff;font-size: 1.4rem;text-align: center;position: relative;padding: 0 5% 5%;}
.service-plan .service-plan-info .plan-type {font-size: 1.5rem;font-family: 'PrometoBold';text-transform: uppercase;display: block;margin: 0 auto;}
.service-plan .service-plan-item-wrapper.highlighted {padding: 0;background-color: transparent;-webkit-box-shadow: -3px 3px 5px 0 rgba(2, 2, 2, 0.11);-moz-box-shadow: -3px 3px 5px 0 rgba(2, 2, 2, 0.11);box-shadow: -3px 3px 5px 0 rgba(2, 2, 2, 0.11);}
.service-plan .service-plan-item-wrapper.highlighted .card-wrapper:first-child {margin-top: 36px;}
.service-plan .service-plan-item-wrapper.highlighted .card-wrapper:first-child ~ .collaps-content .button-wrapper {padding-bottom: 25%;}
.service-plan .service-plan-item-wrapper.highlighted .card-indication ~ .card-wrapper {margin-top: 0;}
.service-plan .service-plan-item-wrapper.highlighted .card-indication ~ .collaps-content .button-wrapper {padding-bottom: 20%;}

 /* Service Plan Corner */
.service-plan .service-plan-corner{font-size: 0;display: block;position: relative;top: 0;margin: 0 auto;width: 62px;}
.service-plan .service-plan-corner-left,
.service-plan .service-plan-corner-right{display: inline-block;overflow: hidden;position: relative;top: 0;width: 20px;height: 20px;}
.service-plan .service-plan-corner-left{right: 0;}
.service-plan .service-plan-corner-right{left: 0;}
.service-plan .service-plan-corner-left:before,
.service-plan .service-plan-corner-right:before{content: '';position: absolute;top: -30px;width: 40px;height: 40px;border: 30px solid #fff;-webkit-border-radius: 100%;-moz-border-radius: 100%;border-radius: 100%;}
.service-plan .service-plan-corner-left:before{left: -50px;}
.service-plan .service-plan-corner-right:before{right: -50px;}

/* Service Plan Notes */
.service-plan-notes {margin: 0 auto;max-width: 91%;}

/* Hovers & Actives */
.touch .service-plan .service-plan-item:hover {/*background: #ececec;*/}
.touch .service-plan .service-plan-item:hover + .service-plan-info .service-plan-corner-left:before,
.touch .service-plan .service-plan-item:hover + .service-plan-info .service-plan-corner-right:before {/*border-color: #ececec;*/}
.touch .service-plan .service-plan-item:hover .card-indication:before {/*background-position: left bottom;*/}
.touch .service-plan .service-plan-item:hover .card-indication:after {/*background-position: right bottom;*/}
.service-plan .service-plan-item.active,
.touch .service-plan .service-plan-item.active:hover {background: #23bb46;}
.service-plan .service-plan-item.active + .service-plan-info:not(.type_3):not(.type_2){background-color: #1b8539;}
.service-plan .service-plan-item.active + .service-plan-info .service-plan-corner-left:before,
.service-plan .service-plan-item.active + .service-plan-info .service-plan-corner-right:before,
.touch .service-plan .service-plan-item.active:hover + .service-plan-info .service-plan-corner-left:before,
.touch .service-plan .service-plan-item.active:hover + .service-plan-info .service-plan-corner-right:before {border-color: #23bb46;}
.service-plan .service-plan-item.active .popular,
.service-plan .service-plan-item.active .type_2,
.touch .service-plan .service-plan-item.active:hover .popular,
.touch .service-plan .service-plan-item.active:hover .type_2 {color: #ffffff;background: #02b245;}
.service-plan .service-plan-item.active .new,
.service-plan .service-plan-item.active .type_3,
.touch .service-plan .service-plan-item.active:hover .new,
.touch .service-plan .service-plan-item.active:hover .type_3 {color: #ffffff;background: #0f5222;}
.service-plan .service-plan-item.active .card-wrapper .plan-name {color: #0f5222;}
.service-plan .service-plan-item.active .card-wrapper .plan-cost {color: #ffffff;}
.service-plan .service-plan-item.active .card-wrapper .plan-cost.old:before{border-bottom-color: #ffffff;}
.service-plan .service-plan-item.active .card-wrapper .plan-cost .feed {color: #0f5222;}
.service-plan .service-plan-item.active .card-wrapper .plan-feed {color: #0f5222;}
.service-plan .service-plan-item.active .card-wrapper .plan-feed sup {color: #ffffff;}
.service-plan .service-plan-item.active .collaps-content .block-collaps-content,
.service-plan .service-plan-item.active .collaps-content ul li.send:first-child {border-top: 1px solid #009b3a;}
.service-plan .service-plan-item.active .collaps-content ul li {color: #ffffff;border-bottom: 1px solid #009b3a;}
.service-plan .service-plan-item.active .collaps-content ul li.send {background: url(images/ic-send-active.png) no-repeat right center;}
.service-plan .service-plan-item.active .collaps-content ul li.send sup{color: #0f5222;}
.service-plan .service-plan-item.active .collaps-content ul li.warranty {background: url(images/ic-warranty-active.png) no-repeat right center;}
.service-plan .service-plan-item.active .collaps-content ul li.campaign {padding: 15px 45px 15px 0;background: url(images/ic-campaign-active.png) no-repeat right center;}
.service-plan .service-plan-item.active .collaps-content .block-collaps-content {border-bottom: 1px solid #009b3a;}
.service-plan .service-plan-item.active .collaps-content .block-collaps-title {color: #0f5222;}
.service-plan .service-plan-item.active .collaps-content .block-collaps-text {color: #ffffff;}
.service-plan .service-plan-item.active .collaps-arrow {background: url(images/ic-collaps-arrow-down-active.png) no-repeat center;}
.service-plan .service-plan-item.active .collaps-arrow.open {background: url(images/ic-collaps-arrow-up-active.png) no-repeat center;}

/**
* Service Plan
* Horizontal
*/
.service-plan.horizontal {}
.service-plan.horizontal .service-plan-item-wrapper {position: relative;display: block;clear: both;width: 100%;margin: 1.72% 0 0 0;}
.service-plan.horizontal .service-plan-item-wrapper:first-child {margin: 0;}
.service-plan.horizontal .service-plan-item .card-cell {width: 50%;clear: none;padding: 3% 2%;}
.service-plan.horizontal .service-plan-item .card-cell + .card-cell {border-left: 1px solid #d8d8da;}
.service-plan.horizontal .service-plan-item.active .card-cell + .card-cell {border-left: 1px solid #009b3a;}
.service-plan.horizontal .service-plan-item .card-wrapper .plan-cost.crossed {font-size: 4rem;}
.service-plan.horizontal .service-plan-item .card-wrapper .plan-offers {display: block;clear: both;padding: 0 0 25px 0;margin: 0 10%;}
.service-plan.horizontal .service-plan-item label {padding: 0;}
.service-plan.horizontal .service-plan-item .card-wrapper {height: 260px;}
.service-plan.horizontal .service-plan-item .collaps-arrow {display: none;}
.service-plan.horizontal .service-plan-item .collaps-content {display: block;}
.service-plan.horizontal .service-plan-item .collaps-content ul {min-height: 0;}

/**
* Service Plan
* Horizontal
* Paper
*/
.service-plan.horizontal.paper .service-plan-item .card-wrapper .plan-cost.crossed .plan-cost-wrapper:before {display: none;}
.service-plan.horizontal.paper .service-plan-item .card-wrapper .plan-cost + .plan-cost {font-size: 3.4rem;color: rgba(35,187,70,0.4);}
.service-plan.horizontal.paper .service-plan-item.active .card-wrapper .plan-cost + .plan-cost {font-size: 3.4rem;color: rgba(255,255,255,0.4);}
.service-plan.horizontal.paper .service-plan-item .card-wrapper .plan-cost sup .icon {display: inline-block;width: 15px;height: 15px;background: url(images/red_alert_icon.png) no-repeat center;background-size: contain;vertical-align: top;}
.service-plan.horizontal.paper .paperIconMessage {display: block;clear: both;padding: 20px 20px 20px 55px;margin: 5% 0 0 0;font-family: Arial, sans-serif;font-size: 1.5rem;line-height: 1.5;color: #515151;font-weight: bold;text-align: left;background: #f8f8f8 url(images/red_alert_icon.png) no-repeat 2% center;}

/* End service-plan */

.accession-panel .accession-main .wizardStep .data-send {margin: 35px 0 0 0;}
.accession-panel .accession-main .wizardStep .data-send .data-send-wrapper {display: block;clear: both;}
.accession-panel .accession-main .wizardStep .data-send .data-send-body {display: block;clear: both;width: 66.5%;padding: 30px 4%;margin: 30px 0 0 0;background: #f5f5f5;-moz-box-sizing: border-box;-webkit-box-sizing: border-box;box-sizing: border-box;}
.accession-panel .accession-main .wizardStep .accession-data-send .data-send .data-send-body {width: auto;padding: 30px 0;}
.accession-panel .accession-main .wizardStep .accession-data-send .accession-data-send-wrapper {padding: 0 2%;}
