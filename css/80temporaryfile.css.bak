/* MultiSelect */

.form-theme-default .form-multiselect-wrapper {
    display:    block;
    clear:      both;
    margin:     20px 0 0 0;
}

.ms-parent {
    position: relative;
    display: block;
    clear: both;
}

.ms-choice {
    cursor: pointer;
    display: block;
    clear: both;
    width: 100%;
    height: 50px;
    line-height: 50px;
    padding: 0;
    overflow: hidden;
    text-align: left;
    white-space: nowrap;
    font-size: 1.5rem;
    color: #495264;
    text-decoration: none;
    background: #f5f5f5;
    border: 0;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
}

    .ms-choice.disabled {
        cursor: not-allowed;
        background: #f4f4f4;
    }

    .ms-choice > span {
        position: absolute;
        top: 0;
        left: 0;
        display: block;
        white-space: nowrap;
        overflow: hidden;
        -ms-text-overflow: ellipsis;
        -o-text-overflow: ellipsis;
        text-overflow: ellipsis;
        padding-left: 20px;
    }

        .ms-choice > span.placeholder {
            color: #495264;
        }

    .ms-choice > div {
        position: absolute;
        top: 0;
        right: 0;
        width: 50px;
        height: 50px;
        border-left: 2px solid #ffffff;
        background: url(images/ddl-arrow-down.png) no-repeat center;
    }

        .ms-choice > div.open {
            background: url(images/ddl-arrow-up.png) no-repeat center;
        }

.ms-drop {
    display: none;
    position: absolute;
    z-index: 1000;
    width: 100%;
    overflow: hidden;
    margin-top: 0;
    padding: 0;
    color: #495264;
    border: 0;
    background: #f5f5f5;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
}

    .ms-drop.bottom {
        top: 100%;
        -webkit-box-shadow: 0 4px 5px rgba(0, 0, 0, .15);
        -moz-box-shadow: 0 4px 5px rgba(0, 0, 0, .15);
        box-shadow: 0 4px 5px rgba(0, 0, 0, .15);
    }

    .ms-drop.top {
        bottom: 100%;
        -webkit-box-shadow: 0 -4px 5px rgba(0, 0, 0, .15);
        -moz-box-shadow: 0 -4px 5px rgba(0, 0, 0, .15);
        box-shadow: 0 -4px 5px rgba(0, 0, 0, .15);
    }

    .ms-drop ul {
        display: block;
        clear: both;
        margin: 0;
        padding: 0;
        overflow: auto;
        list-style: none;
    }

        .ms-drop ul > li {
            display: block;
            clear: both;
            list-style: none;
        }

            .ms-drop ul > li .disabled {
                opacity: .35;
                filter: Alpha(Opacity=35);
            }

            .ms-drop ul > li label {
                cursor: pointer;
                display: block;
                clear: both;
                white-space: nowrap;
                padding: 7px 20px;
                font-size: 1.5rem;
            }

                .ms-drop ul > li label:before {
                    content: '';
                    vertical-align: middle;
                    display: inline-block;
                    position: relative;
                    border: 1px solid #dbdbdb;
                    width: 25px;
                    height: 25px;
                    margin: -4px 0 0 0;
                    -webkit-box-sizing: border-box;
                    -moz-box-sizing: border-box;
                    box-sizing: border-box;
                }

            .ms-drop ul > li.selected label:before {
                background: #02b245 url(images/checkbox-checked.png) no-repeat center;
            }

    .ms-drop input[type="checkbox"] {
        visibility: hidden;
        width: 1px;
        height: 1px;
    }


/* End MultiSelect */

/* interests-list */
.interests-list{margin-top: 20px;}
.interests-list ul{font-size: 0;margin: 0 0 0 -1%;padding: 0;width: 90%;}
.interests-list li{
    position:       relative;
    display:        inline-block;
    vertical-align: top;
    margin:         0 1% 2% 1%;
    text-align:     center;
}
.interests-list li:before{content: '';position: absolute;left: 0;top: 0;width: 100%;height: 100%;background-color: rgba(0,0,0, 0.60);}

.interests-list li label {
    cursor:         pointer;
    display:        block;
    clear:          both;
    width:          140px; 
    height:         140px;
}

.interests-list li .ic{
    position: relative;
    display: block;
    font-family: 'PrometoRegular';
    font-size: 1.8rem;
    color: #ffffff;
    line-height: 1.2;
    padding: 95px 5px 0;
}

.interests-list li .ic:before{
    content:                '';
    position:               absolute;
