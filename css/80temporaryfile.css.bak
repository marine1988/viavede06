/* form-select-wrapper */
.form-theme-default .form-select-wrapper {display: block; clear: both; padding: 0; /*min-height: 80px;*/}
.form-theme-default .form-select-wrapper + .link{/*color: #02b245;*/font-size: 1.4rem;margin-left: 10px;}
.form-theme-default .form-select-wrapper + .link .link-txt{font-size: inherit;}
.touch .form-theme-default .form-select-wrapper + .link:hover{text-decoration: none;}

.form-theme-default .form-select {display: block; clear: both; width: 100%; /*height: 1px;*/} /* nao percebi porque e' que tinha este height: 1px; */
.form-theme-default .form-select .select2-hidden-accessible{width: 1px!important;}

/* Remocao Avalon */
.form-theme-default .form-select-wrapper .select2-container {
    position:               static;
    cursor:                 pointer;
    display:                block;
    clear:                  both;
    width:                  100%!important;
    padding:                0;
    text-decoration:        none;
    background:             #f5f5f5;
    border:                 0;
    -webkit-border-radius:  3px;
    -moz-border-radius:     3px;
    border-radius:          3px;
}

.form-theme-default .form-select-wrapper .select2-container--default .select2-selection--single {
  background:               transparent;
  border:                   0;
  -webkit-border-radius:    0;
  -moz-border-radius:       0;
  border-radius:            0;
}

.form-theme-default .form-select-wrapper .select2-container .select2-selection--single {
    position: relative;
    display:  block;
    clear:    both;
    height:   44px!important;
}

.form-theme-default .form-select-wrapper .select2-container--default .select2-selection--single .select2-selection__rendered {
    position:           absolute;
    top:                0;
    left:               0;
    display:            block;
    width:              70%;
    padding: 8px 0 8px 20px;
    text-align:         left;
    font-size:          1.6rem;
    color:              #495264;
    white-space:        nowrap;
    overflow:           hidden;
    -ms-text-overflow:  ellipsis;
    -o-text-overflow:   ellipsis;
    text-overflow:      ellipsis;
}

.form-theme-default .form-select-wrapper .select2-container--default .select2-selection--single .select2-selection__arrow {
    position: absolute;
    top: 0;
    right: 0;
    width: 42px;
    height: 44px;
    border-left: 2px solid #ffffff;
    background: url(images/ddl-arrow-down.png) no-repeat center;
}

.form-theme-default .form-select-wrapper .select2-container--open .select2-selection--single .select2-selection__arrow {
    background: url(images/ddl-arrow-up.png) no-repeat center;
}

.form-theme-default .form-select-wrapper .select2-container--default .select2-selection--single .select2-selection__arrow b {
    display: none;
}

.form-theme-default .select2-container--default .select2-dropdown{
    left:       0;
    top:        10px;
    margin:     0;
    padding:    0;
    color:      #495264;
    text-transform: none;
    border:     0;
    background: #f5f5f5;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
}

.form-theme-default .select2-container--default .select2-dropdown:before {
    display: none;
}

.form-theme-default .select2-container--default .select2-results__option{text-align: left; padding: 10px 20px;background: none;}
.form-theme-default .select2-container--default .select2-results__option--highlighted[aria-selected]{color: #444444; background: #dddddd;}


/*disabled*/
.form-theme-default .form-select-wrapper .select2-container--default.select2-container--disabled .select2-selection--single,
.form-theme-default .form-select-wrapper .select2-container--default.select2-container--disabled .select2-selection--multiple {
  background-color: #eee;
  cursor: not-allowed;
}

.form-theme-default .form-select-wrapper .select2-container--default.select2-container--disabled .select2-selection--single .select2-selection__rendered,
.form-theme-default .form-select-wrapper .select2-container--default.select2-container--disabled .select2-selection--multiple .select2-selection__rendered {
    color: #999999;
}

.form-theme-default .form-select-wrapper .select2-container--default.select2-container--disabled .select2-selection__arrow {
    background: url(images/ddl-arrow-down_light.png) no-repeat center;
}

